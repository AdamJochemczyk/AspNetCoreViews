@model IEnumerable<zaj9.Models.Lab>

@{
    ViewData["Title"] = "Index2";
}

<h1>Index2</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NazwaLab)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.iloscPracownikow)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.NazwaLab)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.iloscPracownikow)
                </td>
                <td>
                    @await Html.PartialAsync("Probka.cshtml", item.Probki)
                </td>
            </tr>
        }
    </tbody>
</table>